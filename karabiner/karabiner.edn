        ; [:4 :!S8]
        ; [:5 :!S5]
        ; [:q :!Sopen_bracket]
        ; [:w :!S9]
        ; [:e :!S3]
        ; [:r :!Squote]
        ; [:t :quote]
        ; [:o :open_bracket] ; [
        ; [:p :close_bracket] ; ]
        ; [:open_bracket :!Sclose_bracket] ; }
        ; [:caps_lock [:km "Match symbols"]]
        ; [:a :slash]
        ; [:s :!Shyphen]
        ; [:d :backslash]
        ; [:f :hyphen]
        ; [:g :!S4]
        ; [:j :equal_sign]
        ; [:l [:hyphen :!Speriod :spacebar]] ; -> arrow (->)
        ; [:semicolon :semicolon]
        [:z :!Sslash]
        [:x :!Sbackslash]
        ; [:c :!S7]
        ; [:v :!Scomma]
        ; [:b [:slash :slash :spacebar]] ; -> //
      ]
    }
